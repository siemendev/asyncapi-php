{
    "name": "siemendev/asyncapi-php-amqp",
    "type": "library",
    "license": "MIT",
    "minimum-stability": "dev",
    "autoload": {
        "psr-4": {
            "Siemendev\\AsyncapiPhp\\Adapter\\Amqp\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Siemendev\\AsyncapiPhp\\Adapter\\Amqp\\Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Patrick Siemen",
            "email": "post@patrick-siemen.de"
        }
    ],
    "require": {
        "php": "^8.2",
        "siemendev/asyncapi-php": "*",
        "php-amqplib/php-amqplib": "^3.0"
    },
    "require-dev": {
        "phpstan/phpstan": "^2.1",
        "friendsofphp/php-cs-fixer": "^3.75",
        "siemendev/asyncapi-php-generator": "*",
        "siemendev/asyncapi-php-serializer-symfony": "*",
        "phpunit/phpunit": "^12.3"
    },
    "repositories": [
        {
            "type": "path",
            "url": "../../core"
        },
        {
            "type": "path",
            "url": "../../spec"
        },
        {
            "type": "path",
            "url": "../../generator"
        },
        {
            "type": "path",
            "url": "../../serializers/symfony-serializer"
        }
    ]
}
